<%- include('layouts/header.ejs') , {udata:locals.udata} %>

<main class="main mb-4">
    <div class="container">
      <div class="row main-in">
    <div class="card text-center maincard col-md-8 col-12 col-lg-6 " >
        <div class="card-body mainbody">
          <h5 class="card-title mt-4 form-title">RESET PASSWORD</h5>
          <form class="form1 w-100" id="signupform" method="post" action="/signup">
           
            <div class="my-5 field w-75 ">
                <input type="password" class="form-control input " placeholder="" name="password" id="password">
                <label for="password" class="form-label label">New Password</label>
                <p class="error mb-2" id="passerror" style="display: none;"></p>
              </div>
            <div class="my-5 field w-75 ">
                <input type="password" class="form-control input " placeholder="" name="confirmpassword" id="confirmpassword">
                <label for="confirmpassword" class="form-label label">Confirm Password</label>
              <p class="error mb-2" id="confirmpasserror" style="display: none;"></p>
              </div>
            <input type="submit" class="logbtn mt-5 mb-3" value="Reset Password">

          </form>
        </div>
      </div>
    </div>
    </div>
  </main>

<%- include('layouts/footer.ejs')  %>
